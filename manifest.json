{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "HelpMeOut",
  "description": "It records the user's screen",
  "icons": {
    "40": "/images/icon40.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "scripting",
    "tabs",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}